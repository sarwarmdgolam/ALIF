    <script>
        $(document).ready(function () {

            //get button by ID
            $('#submit_contact').submit(function (e) {
                e.preventDefault();

                //call a function with parameters
                $.ajax({
                    url: 'Home/Index',  //(rec)= Controller's-name
                    //(recieveData) = Action's method name
                    type: 'POST',
                    timeout: '12000', //(optional 12 seconds)
                    datatype: 'text',
                    data: {
                        //Get the input from Document Object Model
                        //by their ID
                        name: contact_form.name.value,
                        email: contact_form.email.value,
                        subject: contact_form.subject.value,
                        message: contact_form.message.value
                    },
                    success: function (data) {

                        if (data.Message == undefined) {
                            // Use data like a partial
                        } else {
                            // Use data.Message for the message and data.AjaxReturn for the partial
                        }
                    },
                    error: function (message) {
                        logOut($self, message);
                    }
                });
                return false;
            });

        });

    </script>
